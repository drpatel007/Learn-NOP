@model Nop.Web.Areas.Admin.CustomModels.Employees.EmployeeModel

<div class="panel-body">

    <div class="form-group">
        <div class="col-md-3">
            <nop-label asp-for="Name" />
        </div>
        <div class="col-md-9">
            <nop-editor asp-for="Name" />
            <span asp-validation-for="Name"></span>
        </div>
    </div>

    <div class="form-group">
        <div class="col-md-3">
            <nop-label asp-for="Salary" />
        </div>
        <div class="col-md-9">
            <nop-editor asp-for="Salary" />
            <span asp-validation-for="Salary"></span>
        </div>
    </div>

    <div class="form-group">
        <div class="col-md-3">
            <nop-label asp-for="DateOfBirth" />
        </div>
        <div class="col-md-9">
            <nop-editor asp-for="DateOfBirth" />
            <span asp-validation-for="DateOfBirth"></span>
        </div>
    </div>

    <div class="form-group">
        <div class="col-md-3">
            <nop-label asp-for="Active" />
        </div>
        <div class="col-md-9">
            <nop-editor asp-for="Active" />
            <span asp-validation-for="Active"></span>
        </div>
    </div>

</div>

<nop-alert asp-alert-id="statesAlert" asp-alert-message="@T("Admin.Common.Alert.States.Failed")" />